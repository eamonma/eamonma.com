background-clip: text;\n    color: transparent;\n    animation: rainbow_animation 6s ease-in-out infinite;\n    background-size: 400% 100%;\n}\n\n@keyframes rainbow_animation {\n    0%,100% {\n        background-position: 0 0;\n    }\n\n    50% {\n        background-position: 100% 0;\n    }\n}\n\nbody {\n    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);\n    font-family: 'Noto Serif', serif;\n    background-color: #F7F4EA;\n    display: flex;\n    flex-direction: column;\n    position: absolute;\n    top: 0;\n    right: 0;\n    bottom: 0;\n    left: 0;\n    margin: 0;\n    padding: 0;\n\n    justify-content: center;\n    align-items: center;\n    width: 100%;\n    height: 100%;\n}\n\nmain {\n    margin-top: -10%;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n}\n\nh1 {\n    font-size: 2em;\n    color: #32312F;\n    text-align: center;\n}\n\nul {\n    list-style: none;\n    display: flex;\n    margin: 0;\n    padding: 0;\n}\n\na {\n    text-decoration: none;\n    color: #32312F;\n}\n\n.social ul>li {\n    margin: 0 1em;\n}\n\n.social ul>li,\n.social ul>li>a {\n    display: block;\n    height: 3.9em;\n    width: 2.8em;\n    background-size: contain;\n    background-repeat: no-repeat;\n    transition: transform 0.2s cubic-bezier(0.645, 0.045, 0.355, 1);\n}\n\n.social ul>li>a {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    position: relative;\n}\n\n.social ul>li>a::after {\n    font-size: .8em;\n    position: absolute;\n    bottom: -.5em;\n    display: block;\n    transition: all 0.2s cubic-bezier(0.645, 0.045, 0.355, 1);\n    opacity: 0;\n    text-transform: uppercase;\n    letter-spacing: .2px;\n}\n\n.social ul>li>a:hover::after {\n    opacity: 1;\n}\n\n.github a {\n    background-image: url(\"assets/github.png\");\n}\n\n.github a::after {\n    content: \"GitHub\";\n}\n\n.email a {\n    background-image: url(\"assets/email.png\");\n}\n\n.email a::after {\n    content: \"Email\";\n}\n\n.instagram a {\n    background-image: url(\"assets/instagram.png\");\n}\n\n.instagram a::after {\n    content: \"Instagram\";\n}\n\n.linkedin a {\n    background-image: url(\"assets/linkedin.png\");\n}\n\n.linkedin a::after {\n    content: \"LinkedIn\";\n}\n\nfooter {\n    position: absolute;\n    bottom: 2em;\n    font-size: 0.8em;\n}\n\n@media (hover: hover) {\n    .social ul>li>a:hover {\n        transform: scale(1.1);\n    }\n\n}\n\n@media (hover: none),\n(max-width: 600px) {\n    body {\n        font-size: 110%;\n    }\n\n    h1 {\n        font-size: 2em;\n        margin-bottom: 0;\n    }\n\n    .social ul>li>a::after {\n        opacity: 1;\n    }\n\n    main,\n    .social,\n    .social ul {\n        width: 100%;\n        padding: 0;\n        margin: 0;\n    }\n\n    .social ul {\n        flex-wrap: wrap;\n        justify-content: space-evenly;\n    }\n\n    .social ul>li {\n        margin: 1.4em 0;\n        flex: 0 50%;\n        display: flex;\n        justify-content: center;\n    }\n\n    .social ul>li,\n    .social ul>li>a {\n        width: 3em;\n        height: 4em;\n    }\n\n    .social ul>li>a::after {\n        bottom: -1em;\n    }\n}"},"lineCount":null}},"error":null,"hash":"ed82ca6fd14007348ea2e3f177a947a3","cacheData":{"env":{}}}
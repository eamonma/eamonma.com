t-style: none;\n    display: flex;\n    margin: 0;\n    padding: 0;\n}\n\na {\n    text-decoration: none;\n    color: #32312F;\n}\n\n.social {\n    margin-top: 1em;\n}\n\n.social ul:hover li {\n    opacity: 0.5;\n}\n\n.social ul li:hover {\n    opacity: 1;\n}\n\n.social ul>li {\n    margin: 0 1em;\n}\n\n.social ul>li,\n.social ul>li>a {\n    display: block;\n    height: 3.9em;\n    width: 3em;\n    background-size: contain;\n    background-repeat: no-repeat;\n    transition: all 0.2s cubic-bezier(0.645, 0.045, 0.355, 1);\n}\n\n.social ul>li>a {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    position: relative;\n}\n\n.social ul>li>a::after {\n    font-size: .8em;\n    position: absolute;\n    bottom: -.8em;\n    display: block;\n    transition: all 0.2s cubic-bezier(0.645, 0.045, 0.355, 1);\n    opacity: 0;\n    text-transform: uppercase;\n    letter-spacing: .2px;\n}\n\n.social ul>li>a:hover::after {\n    opacity: 1;\n}\n\n.github a {\n    background-image: url(\"assets/github.png\");\n}\n\n.github a::after {\n    content: \"GitHub\";\n}\n\n.email a {\n    background-image: url(\"assets/email.png\");\n}\n\n.email a::after {\n    content: \"Email\";\n}\n\n.instagram a {\n    background-image: url(\"assets/instagram.png\");\n}\n\n.instagram a::after {\n    content: \"Instagram\";\n}\n\n.linkedin a {\n    background-image: url(\"assets/linkedin.png\");\n}\n\n.linkedin a::after {\n    content: \"LinkedIn\";\n}\n\nfooter {\n    /* position: relative;\n    bottom: -10em;\n    font-size: 0.8em; */\n    display: none;\n}\n\n.hidden {\n    visibility: hidden;\n}\n\n@media (hover: hover) {\n    .social ul>li>a:hover {\n        transform: scale(1.1);\n    }\n\n}\n\n@media (hover: none),\n(max-width: 600px) {\n    body {\n        font-size: 110%;\n    }\n\n    h1 {\n        font-size: 2em;\n        margin-bottom: 0;\n        width: 100%;\n    }\n\n    .type {\n        margin: 0 10px;\n    }\n\n    .typed-cursor {\n        margin-left: -13px;\n    }\n\n    .social ul>li>a::after {\n        opacity: 1;\n    }\n\n    main,\n    .social,\n    .social ul {\n        width: 100%;\n        padding: 0;\n        margin: 0;\n    }\n\n    .social ul {\n        flex-wrap: wrap;\n        justify-content: space-evenly;\n    }\n\n    .social ul>li {\n        margin: 1.4em 0;\n        flex: 0 50%;\n        display: flex;\n        justify-content: center;\n    }\n\n    .social ul>li,\n    .social ul>li>a {\n        width: 3em;\n        height: 4em;\n    }\n\n    .social ul>li>a::after {\n        bottom: -1em;\n    }\n}\n\n.animated {\n    animation-duration: 0.7s;\n}"},"lineCount":null}},"error":null,"hash":"a2e16713fea08a3d0e04a981dbb6b765","cacheData":{"env":{}}}